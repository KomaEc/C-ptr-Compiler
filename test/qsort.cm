
void qsort(int l, int r, int[] a) {
    if (l >= r) return ;
    int pivot = a[l];
    int i = l;
    int j = r;
    while(i < j) {
        while(i < j && a[j] >= pivot) { j = j - 1; }
        a[i] = a[j];
        if (i < j) {
            while(i < j && a[i] <= pivot) { i = i + 1; }
            a[j] = a[i];
        }
    }
    a[i] = pivot;
    qsort(l, i - 1, a);
    qsort(i + 1, r, a);
    return ;

}

void quick(int[] a, int l, int r) {
    if (l >= r) return;
    int i = l + 1;
    int j = r;
    int pivot = a[l];
    while (i < j) {
        int temp = a[i];
        a[i] = a[j];
        a[j] = temp;
        while (i < j && a[i] < pivot) { i = i + 1; }
        while (a[j] > pivot) { j = j - 1; }
    }
    a[j] = pivot;
    quick(a, l, j - 1);
    quick(a, j + 1, r);
    return;
}

int main() {
    int[] a = new int[100];
    qsort(0, 99, a);
    return 0;
}